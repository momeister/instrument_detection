import wave

# Öffnen Sie die .wav-Datei im Lesemodus
with wave.open("Klänge\einstimmig-1.wav", "rb") as wavfile:
    # Erhalten Sie die Anzahl der Kanäle (Mono oder Stereo)
    channels = wavfile.getnchannels()

    # Erhalten Sie die Abtastrate (Sample Rate) in Hz
    sample_rate = wavfile.getframerate()

    print(f"Anzahl der Kanäle: {channels}")
    print(f"Abtastrate: {sample_rate} Hz")
